{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noResolve": false,
    // "noImplicitAny": false,
    "strict": true,
    "declaration": true,                 
    "sourceMap": true,
    "preserveConstEnums":true,
    "lib": [
        "dom",
        "es6"
    ],
    "rootDir": "src",
    "outDir": "./dist",
    "skipLibCheck": true,                           /* Skip type checking of declaration files. */
  },
  "include": ["src"]
}